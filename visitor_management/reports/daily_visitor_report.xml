        <t t-call="web.basic_layout">
            <div class="page">
                <p>Visitors</p>
                <t t-foreach="name" t-as="nam">
                    <span>
                        <t t-out="nam"/>
                    </span>
                </t>

                <p>refrence employee</p>
                <t t-foreach="refrence_emp" t-as="ref">
                    <span>
                        <t t-out="ref"/>
                    </span>
                </t>
            </div>
        </t>
    </template>

</odoo>